<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ddal-rule PUBLIC "-//suning.com//DTD ddal-rule//EN" "http://suning.com/dtd/ddal-rule.dtd">
<ddal-rule>

	<tableRouter id="order_group">
		<partition>
			<table shard="shard0" suffixes="_01,_02,_03,_04"/>
			<table shard="shard1" suffixes="_01,_02,_03,_04"/>
			<table shard="shard2" suffixes="_01,_02,_03,_04"/>
			<table shard="shard3" suffixes="_01,_02,_03,_04"/>
		</partition>
		<tableRule>
			<![CDATA[ ${order_id} % 16]]>
		</tableRule>
	</tableRouter>
	
	<tableRouter id="customer_group">
		<partition>
			<table shard="shard0" suffixes="_01,_02"/>
			<table shard="shard1" suffixes="_01,_02"/>
			<table shard="shard2" suffixes="_01,_02"/>
			<table shard="shard3" suffixes="_01,_02"/>			
		</partition>
		<tableRule>
			<![CDATA[ ${customer_id} % 8]]>
		</tableRule>
	</tableRouter>
</ddal-rule>